local function BZOC_fake_script() -- ScreenGui.Handler 
	local script = Instance.new('LocalScript', ScreenGui)

	PLAYER = game.Players.LocalPlayer
	MOUSE = PLAYER:GetMouse()
	CC = game.Workspace.CurrentCamera

	ENABLED = false
	ESP_ENABLED = false

	_G.FREE_FOR_ALL = true

	_G.ESP_BIND = 52

	_G.AIM_AT = "UpperTorso"

	wait(1)

	function GetNearestPlayerToMouse()
		local PLAYERS = {}
		local PLAYER_HOLD = {}
		local DISTANCES = {}
		for i, v in pairs(game.Players:GetPlayers()) do
			if v ~= PLAYER then
				table.insert(PLAYERS, v)
			end
		end
		for i, v in pairs(PLAYERS) do
			if _G.FREE_FOR_ALL == false then
				if v and (v.Character) ~= nil and v.TeamColor ~= PLAYER.TeamColor then
					local AIM = v.Character:FindFirstChild(_G.AIM_AT)
					if AIM ~= nil then
						local DISTANCE = (AIM.Position - game.Workspace.CurrentCamera.CoordinateFrame.p).magnitude
						local RAY =
							Ray.new(
								game.Workspace.CurrentCamera.CoordinateFrame.p,
								(MOUSE.Hit.p - CC.CoordinateFrame.p).unit * DISTANCE
							)
						local HIT, POS = game.Workspace:FindPartOnRay(RAY, game.Workspace)
						local DIFF = math.floor((POS - AIM.Position).magnitude)
						PLAYER_HOLD[v.Name .. i] = {}
						PLAYER_HOLD[v.Name .. i].dist = DISTANCE
						PLAYER_HOLD[v.Name .. i].plr = v
						PLAYER_HOLD[v.Name .. i].diff = DIFF
						table.insert(DISTANCES, DIFF)
					end
				end
			elseif _G.FREE_FOR_ALL == true then
				local AIM = v.Character:FindFirstChild(_G.AIM_AT)
				if AIM ~= nil then
					local DISTANCE = (AIM.Position - game.Workspace.CurrentCamera.CoordinateFrame.p).magnitude
					local RAY =
						Ray.new(
							game.Workspace.CurrentCamera.CoordinateFrame.p,
							(MOUSE.Hit.p - CC.CoordinateFrame.p).unit * DISTANCE
						)
					local HIT, POS = game.Workspace:FindPartOnRay(RAY, game.Workspace)
					local DIFF = math.floor((POS - AIM.Position).magnitude)
					PLAYER_HOLD[v.Name .. i] = {}
					PLAYER_HOLD[v.Name .. i].dist = DISTANCE
					PLAYER_HOLD[v.Name .. i].plr = v
					PLAYER_HOLD[v.Name .. i].diff = DIFF
					table.insert(DISTANCES, DIFF)
				end
			end
		end

		if unpack(DISTANCES) == nil then
			return false
		end

		local L_DISTANCE = math.floor(math.min(unpack(DISTANCES)))
		if L_DISTANCE > 20 then
			return false
		end

		for i, v in pairs(PLAYER_HOLD) do
			if v.diff == L_DISTANCE then
				return v.plr
			end
		end
		return false
	end

	local GUI_TARGET = script.Parent.target

	local GUI_AIM_AT = script.Parent.Main.body.TextLabel

	local TRACK = false

	function FIND()
		CLEAR()
		TRACK = true
		spawn(
			function()
				while wait() do
					if TRACK then
						CLEAR()
						for i, v in pairs(game.Players:GetChildren()) do
							if v.Character and v.Character:FindFirstChild("UpperTorso") then
								if _G.FREE_FOR_ALL == false then
									if v.TeamColor ~= PLAYER.TeamColor then
										if v.Character:FindFirstChild("UpperTorso") then
											CREATE(v.Character.UpperTorso, true)
										end
									end
								else
									if v.Character:FindFirstChild("UpperTorso") then
										CREATE(v.Character.UpperTorso, true)
									end
								end
							end
						end
					end
				end
				wait(1)
			end
		)
	end

	MOUSE.Button2Down:connect(
		function()
			ENABLED = true
		end
	)

	MOUSE.Button2Up:connect(
		function()
			ENABLED = false
		end
	)

	game:GetService("RunService").RenderStepped:connect(
	function()
		if ENABLED then
			local TARGET = GetNearestPlayerToMouse()
			if (TARGET ~= false) then
				local AIM = TARGET.Character:FindFirstChild(_G.AIM_AT)
				if AIM then
					CC.CoordinateFrame = CFrame.new(CC.CoordinateFrame.p, AIM.CFrame.p)
				end
				GUI_TARGET.Text = "Target : " .. TARGET.Name:sub(1, 5)
			else
				GUI_TARGET.Text = "Target : nil"
			end
		end
	end
	)

end
coroutine.wrap(BZOC_fake_script)()
local function QLFLSPS_fake_script() -- Open.LocalScript 
	local script = Instance.new('LocalScript', Open)

	local button = script.Parent
	local object = script.Parent.Parent.Main
	local originalSize = object.Size
	local isBig = false
	local timeout = 3

	function onClick()
		if isBig then
			object:TweenSize(originalSize, "Out", "Quad", 0.5, true)
			wait(0.5)
			object:TweenSize(UDim2.new(0.002, 0,0.008, 0), "In", "Quad", 0.5, true)
			isBig = false
			wait(0.53)
			object.Visible = false
		else
			object:TweenSize(UDim2.new(0.001, 0,0.241, 0), "In", "Quad", 0.5, true)
			object.Visible = true
			isBig = true
			wait(0.5)
			object:TweenSize(UDim2.new(0.184, 0,0.241, 0), "In", "Quad", 0.5, true)
		end
		button.Enabled = false
		wait(timeout)
		button.Enabled = true
	end

	button.MouseButton1Click:Connect(onClick)

end
coroutine.wrap(QLFLSPS_fake_script)()
